Q2.#include <stdio.h>
int main() {
    int a, b, temp;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    temp = a;
    a = b;
    b = temp;

    printf("After swapping: a = %d, b = %d\n", a, b);
    return 0;
}
Q3.
#include <stdio.h>
int main() {
    int a, b;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    a = a + b;
    b = a - b;
    a = a - b;

    printf("After swapping: a = %d, b = %d\n", a, b);
    return 0;
}
Q4.
#include <stdio.h>
int main() {
    int n, last, except_last;
    printf("Enter a number: ");
    scanf("%d", &n);

    last = n % 10;
    except_last = n / 10;

    printf("Last digit = %d\n", last);
    printf("All digits except last = %d\n", except_last);

    return 0;
}
Q5.
#include <stdio.h>
int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    printf("Square = %d\n", n * n);
    printf("Cube = %d\n", n * n * n);

    return 0;
}