Q1.
#include <stdio.h>
int main() {
    int a, b;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    if (a == b)
        printf("True (Numbers are equal)\n");
    else
        printf("False (Numbers are not equal)\n");

    return 0;
}
Q2.
#include <stdio.h>
int main() {
    int n;

    printf("Enter a number: ");
    scanf("%d", &n);

    if (n > 0)
        printf("The number is Positive\n");
    else if (n < 0)
        printf("The number is Negative\n");
    else
        printf("The number is Zero\n");

    return 0;
}
Q4.
#include <stdio.h>
int main() {
    int a, b, c;

    printf("Enter three numbers: ");
    scanf("%d %d %d", &a, &b, &c);

    if (a >= b && a >= c)
        printf("Greatest number = %d\n", a);
    else if (b >= a && b >= c)
        printf("Greatest number = %d\n", b);
    else
        printf("Greatest number = %d\n", c);

    return 0;
}